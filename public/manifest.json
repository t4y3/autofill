{
  "manifest_version": 2,
  "name": "Autofill",
  "description": "Automatically enter form (text, textarea, select, checkbox, radio)",
  "version": "1.0",
  "browser_action": {
    "default_title": "Autofill",
    "default_popup": "./build/index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["sample.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "https://*/*",
    "http://*/*",
    "storage"
  ]
}